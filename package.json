{
  "name": "@overmindtech/sdp-js",
  "version": "5.2.1",
  "description": "Javascript and Typescript libraries for the State Description Protocol",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepack": "pnpm build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier:check": "prettier ./src --check",
    "prettier:fix": "prettier ./src --write",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run --coverage",
    "codequality:check": "pnpm lint && pnpm prettier:check && pnpm typecheck",
    "codequality:fix": "pnpm lint:fix && pnpm prettier:fix"
  },
  "dependencies": {
    "@bufbuild/connect-query": "^0.2.1",
    "@bufbuild/protobuf": "^1.2.0",
    "@tanstack/react-query": "^4.20.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.0",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "changelogen": "^0.5.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-unjs": "^0.1.0",
    "jest-websocket-mock": "^2.4.0",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.2.1",
    "vitest": "^0.31.0"
  },
  "packageManager": "pnpm@8.4.0"
}
