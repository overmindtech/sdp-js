{
  "name": "@overmindtech/sdp-js",
  "version": "4.1.0",
  "description": "Javascript and Typescript libraries for the State Description Protocol",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepack": "pnpm build",
    "test": "jest",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "prettier:check": "prettier ./src --check",
    "prettier:fix": "prettier ./src --write",
    "codequality:check": "pnpm lint && pnpm prettier:check && pnpm typecheck",
    "codequality:fix": "pnpm lint:fix && pnpm prettier:fix"
  },
  "keywords": [],
  "author": "Dylan Ratcliffe",
  "license": "ISC",
  "dependencies": {
    "@bufbuild/protobuf": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.14.4",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-websocket-mock": "^2.4.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "unbuild": "^1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overmindtech/sdp-js.git"
  },
  "bugs": {
    "url": "https://github.com/overmindtech/sdp-js/issues"
  },
  "homepage": "https://github.com/overmindtech/sdp-js#readme",
  "peerDependencies": {
    "@babel/core": "^7.21.0"
  }
